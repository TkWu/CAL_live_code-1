<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/root"
    android:layout_width="340px"
    android:layout_height="560.7px"
    android:background="@drawable/bg_flight_tab_flight_detial_inside_line"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/ll_flight_detial_flight_status"
        android:layout_width="338px"
        android:layout_height="266px"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="1px"
        android:background="@drawable/bg_flight_tab_flight_detial_up_part"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="7px"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/iv_on_time"
                android:layout_width="24px"
                android:layout_height="24px"
                android:layout_gravity="center_vertical"
                android:scaleType="fitXY"
                android:src="@drawable/ic_on_time" />

            <TextView
                android:id="@+id/tv_flight_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="center"
                android:paddingLeft="7px"
                android:paddingRight="7px"
                android:text="@string/flight_status_arrived_on_time"
                android:textColor="@color/emerald"
                android:textSize="16px"
                android:maxLines="1"
                android:ellipsize="end"
                style="@style/medium" />
        </LinearLayout>

        <View
            android:layout_width="300px"
            android:layout_height="1px"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="10.7px"
            android:alpha="0.6"
            android:background="@color/warm_grey_50" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20px"
            android:layout_marginRight="20px"
            android:layout_marginTop="20px"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:id="@+id/tv_departure_location"
                    android:layout_width="130px"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:gravity="left|center_vertical"
                    android:text="Taipei"
                    android:textColor="@color/black_one"
                    android:textSize="20px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/regular" />

                <TextView
                    android:id="@+id/tv_arrive_location"
                    android:layout_width="130px"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:gravity="right|center_vertical"
                    android:text="Fukuoka"
                    android:textColor="@color/black_one"
                    android:textSize="20px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/regular" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:id="@+id/tv_departure_time"
                    android:layout_width="130px"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:gravity="left|center_vertical"
                    android:text="08:10"
                    android:textColor="@color/french_blue"
                    android:textSize="40px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/regular" />

                <ImageView
                    android:id="@+id/iv_ic_flight_from_to"
                    android:layout_width="30px"
                    android:layout_height="30px"
                    android:layout_centerInParent="true"
                    android:background="@drawable/ic_flight_from_to" />

                <TextView
                    android:id="@+id/tv_arrive_time"
                    android:layout_width="130px"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:gravity="right|center_vertical"
                    android:text="11:20"
                    android:textColor="@color/french_blue"
                    android:textSize="40px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/regular" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:id="@+id/tv_departure_date"
                    android:layout_width="130px"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:gravity="left|center_vertical"
                    android:text="Sat, Oct 17, 2015"
                    android:textColor="@color/french_blue"
                    android:textSize="13px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/light" />

                <TextView
                    android:id="@+id/tv_arrive_date"
                    android:layout_width="130px"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:gravity="right|center_vertical"
                    android:text="Sun, Oct 18, 2015"
                    android:textColor="@color/french_blue"
                    android:textSize="13px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/light" />
            </RelativeLayout>
        </LinearLayout>
        <!--<View-->
        <!--android:id="@+id/vline_meal"-->
        <!--android:layout_width="300px"-->
        <!--android:layout_height="1px"-->
        <!--android:layout_gravity="center_horizontal"-->
        <!--android:layout_marginTop="21px"-->
        <!--android:alpha="0.39"-->
        <!--android:background="@drawable/bg_passenger_card_line_50" />-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20px"
            android:layout_marginRight="20px"
            android:layout_marginTop="30px"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:id="@+id/tv_departure_abbreviation"
                    android:layout_width="130px"
                    android:layout_height="19px"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:gravity="left|center_vertical"
                    android:text="TPE"
                    android:textColor="@color/black_one"
                    android:textSize="16px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/regular" />

                <TextView
                    android:id="@+id/tv_arrive_abbreviation"
                    android:layout_width="130px"
                    android:layout_height="19px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:gravity="right|center_vertical"
                    android:text="FUK"
                    android:textColor="@color/black_one"
                    android:textSize="16px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/regular" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="2px">

                <TextView
                    android:id="@+id/tv_departure_location_detial"
                    android:layout_width="130px"
                    android:layout_height="16px"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:gravity="left|center_vertical"
                    android:text="Taiwan Taoyuan Int’l"
                    android:textColor="@color/greyish_brown"
                    android:textSize="13px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/light" />

                <TextView
                    android:id="@+id/tv_arrive_location_detial"
                    android:layout_width="130px"
                    android:layout_height="16px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:gravity="right|center_vertical"
                    android:text="Fukuoka Apt"
                    android:textColor="@color/greyish_brown"
                    android:textSize="13px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/light" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="3px">

                <TextView
                    android:id="@+id/tv_departure_terminal"
                    android:layout_width="130px"
                    android:layout_height="16px"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:gravity="left|center_vertical"
                    android:text="@string/home_terminal"
                    android:textColor="@color/greyish_brown"
                    android:textSize="13px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/light" />

                <TextView
                    android:id="@+id/tv_arrive_terminal"
                    android:layout_width="130px"
                    android:layout_height="16px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:gravity="right|center_vertical"
                    android:text="@string/home_terminal"
                    android:textColor="@color/greyish_brown"
                    android:textSize="13px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/light" />
            </RelativeLayout>

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="338px"
        android:layout_height="292.7px"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/bg_flight_tab_flight_detial_down_part"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="20px"
            android:layout_marginRight="20px"
            android:layout_marginTop="25.3px"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:id="@+id/tv_flight_detial_airlines_title"
                    android:layout_width="130px"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:gravity="left|center_vertical"
                    android:text="@string/trips_detail_airlines"
                    android:textColor="@color/warm_grey_two"
                    android:textSize="16px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/light" />

                <TextView
                    android:id="@+id/tv_flight_detial_airlines_value"
                    android:layout_width="130px"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:gravity="right|center_vertical"
                    android:text="china airlines"
                    android:textColor="@color/black_one"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textSize="16px"
                    style="@style/light" />
            </RelativeLayout>

            <View
                android:id="@+id/vline_meal2"
                android:layout_width="300px"
                android:layout_height="1px"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="16px"
                android:alpha="0.39"
                android:background="@drawable/bg_passenger_card_line_50" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="16px">

                <TextView
                    android:id="@+id/tv_flight_detial_flight_number_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:gravity="left|center_vertical"
                    android:text="@string/trips_detail_flight_number"
                    android:textColor="@color/warm_grey_two"
                    android:textSize="16px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/light" />

                <TextView
                    android:id="@+id/tv_flight_detial_flight_number_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:gravity="right|center_vertical"
                    android:text="CI 65"
                    android:textColor="@color/black_one"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textSize="16px"
                    style="@style/light" />
            </RelativeLayout>


            <View
                android:id="@+id/vline_meal3"
                android:layout_width="300px"
                android:layout_height="1px"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="16px"
                android:alpha="0.39"
                android:background="@drawable/bg_passenger_card_line_50" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="16px">

                <TextView
                    android:id="@+id/tv_flight_detial_travel_time_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:gravity="left|center_vertical"
                    android:text="@string/trips_detail_travel_time"
                    android:textColor="@color/warm_grey_two"
                    android:textSize="16px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/light" />

                <TextView
                    android:id="@+id/tv_flight_detial_travel_time_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:gravity="right|center_vertical"
                    android:text="3h 30m"
                    android:textColor="@color/black_one"
                    android:textSize="16px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/light" />
            </RelativeLayout>

            <View
                android:id="@+id/vline_meal4"
                android:layout_width="300px"
                android:layout_height="1px"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="16px"
                android:alpha="0.39"
                android:background="@drawable/bg_passenger_card_line_50" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="16px">

                <TextView
                    android:id="@+id/tv_flight_detial_reservation_no_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:gravity="left|center_vertical"
                    android:text="@string/trips_detail_reference_no"
                    android:textColor="@color/warm_grey_two"
                    android:textSize="16px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/light" />

                <TextView
                    android:id="@+id/tv_flight_detial_reservation_no_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:gravity="right|center_vertical"
                    android:text="K6D7F4"
                    android:textColor="@color/black_one"
                    android:textSize="16px"
                    android:maxLines="1"
                    android:ellipsize="end"
                    style="@style/light" />
            </RelativeLayout>

            <View
                android:id="@+id/vline_meal5"
                android:layout_width="300px"
                android:layout_height="1px"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="16px"
                android:alpha="0.39"
                android:background="@drawable/bg_passenger_card_line_50" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20px"
                android:layout_marginBottom="20px">

                <RelativeLayout
                    android:id="@+id/rl_flight_status_click"
                    android:layout_width="180px"
                    android:layout_height="40px"
                    android:layout_alignParentLeft="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/bg_transparent_press_black20"
                    android:clickable="true">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/bg_btn_flight_tab_flight_status_down_part" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:orientation="horizontal">

                        <!-- 644336 2019 2月3月 行事曆 -->
                        <!--<ImageView-->
                        <!--android:id="@+id/iv_ic_flight_status_b"-->
                        <!--android:layout_width="24px"-->
                        <!--android:layout_height="24px"-->
                        <!--android:layout_gravity="center_vertical"-->
                        <!--android:src="@drawable/ic_flight_status_b" />-->

                        <!--<TextView-->
                        <!--android:layout_width="wrap_content"-->
                        <!--android:layout_height="wrap_content"-->
                        <!--android:layout_gravity="center_vertical"-->
                        <!--android:layout_marginLeft="7px"-->
                        <!--android:gravity="center_vertical"-->
                        <!--android:text="@string/flight_status"-->
                        <!--android:textColor="@color/french_blue"-->
                        <!--android:textSize="14px"-->
                        <!--android:maxLines="1"-->
                        <!--android:ellipsize="end"-->
                        <!--style="@style/regular" />-->
                        <ImageView
                            android:id="@+id/iv_ic_add_to_calendar"
                            android:layout_width="24px"
                            android:layout_height="24px"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/ic_add_to_calendar" />
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="7px"
                            android:gravity="center_vertical"
                            android:text="@string/add_to_calendar"
                            android:textColor="@color/french_blue"
                            android:textSize="14px"
                            android:maxLines="1"
                            android:ellipsize="end"
                            style="@style/regular" />
                        <!-- 644336 2019 2月3月 行事曆 -->
                    </LinearLayout>
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_share_click"
                    android:layout_width="100px"
                    android:layout_height="40px"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/bg_transparent_press_black20"
                    android:clickable="true">

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@drawable/bg_btn_flight_tab_flight_status_down_part" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerInParent="true"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/iv_ic_share_b"
                            android:layout_width="24px"
                            android:layout_height="24px"
                            android:layout_gravity="center_vertical"
                            android:src="@drawable/ic_share_b" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="6.7px"
                            android:gravity="center_vertical"
                            android:text="@string/share"
                            android:textColor="@color/french_blue"
                            android:textSize="14px"
                            android:maxLines="1"
                            android:ellipsize="end"
                            style="@style/regular" />

                    </LinearLayout>
                </RelativeLayout>
            </RelativeLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>